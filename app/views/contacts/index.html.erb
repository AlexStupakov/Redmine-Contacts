<p id="notice"><%= notice %></p>

<h1>Contacts</h1>

<table>
  <thead>
    <tr>
      <th>Project</th>
      <th>Name</th>
      <th>Country</th>
      <th>City</th>
      <th>Street</th>
      <th>House</th>
      <th>Phone</th>
      <th>Zip</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.project_id %></td>
        <td><%= contact.name %></td>
        <td><%= contact.country %></td>
        <td><%= contact.city %></td>
        <td><%= contact.street %></td>
        <td><%= contact.house %></td>
        <td><%= contact.phone %></td>
        <td><%= contact.zip %></td>
        <td><%= contact.email %></td>
        <td><%= link_to 'Show', contact %></td>
        <td><%= link_to 'Edit', edit_contact_path(contact) %></td>
        <td><%= link_to 'Destroy', contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contact', new_contact_path %>
